                                    <elevation type="parapet" height="1.2" width="0.5" inset="1" skin_name="parapet.poured">
                                        <roof type="flat" skin_name="parapet.concrete"/>
                                    </elevation>
                                </elevations>
                            </elevation>
                        </elevations>
                    </elevation>
                </elevations>
            </elevation>
        </elevations>
   </building>


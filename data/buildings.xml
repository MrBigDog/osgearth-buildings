   <building name="Commercial 2-tier building" tags="commercial" min_height="50">
        <skin_tags>commercial facade</skin_tags>
        <elevations>
            <elevation height_percentage="95">
                <roof type="flat" skin_tags="roof commercial"/>
                <elevations>                
                    <elevation height="5" inset="3">
                        <roof type="flat" skin_tags="roof commercial" model_name="roof.ac"/>
                        <elevations>
                            <elevation type="parapet" height="1.2" width="0.5" inset="1" skin_name="parapet.poured">
                                <roof type="flat" skin_name="parapet.concrete"/>
                            </elevation>
                        </elevations>
                    </elevation>
                </elevations>
            </elevation>
        </elevations>
   </building>
   
   <building name="Commercial 3-tier building offset" tags="commercial" min_height="80">
        <skin_tags>commercial facade</skin_tags>
        <elevations>
            <elevation height_percentage="15">
                <roof type="flat" skin_tags="roof commercial"/>
                <elevations>                
                    <elevation height_percentage="40" inset="5" xoffset="-3" yoffset="-3">
                        <roof type="flat" skin_tags="roof commercial"/>
